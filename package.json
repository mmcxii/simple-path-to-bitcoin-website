  "main": "index.js",
  "name": "project-template-v1",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:browser": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "clean": "rimraf dist",
    "commit-lint": "commitlint --config ./.commitlintrc.json --edit",
    "docker:start:dev": "docker-compose -f ./tools/docker/docker-compose.dev.yml up",
    "docker:start:dev:detach": "npm run docker:start:dev -- --detach",
    "husky:commit-msg": "npm run commit-lint",
    "husky:pre-commit": "npm run lint-staged",
    "husky:pre-push": "npm run test",
    "lint-staged": "lint-staged --concurrent false --config ./.lintstagedrc.json",
    "lint-staged:lint": "eslint --fix",
    "lint-staged:prettier": "prettier --write",
    "lint-staged:sortier": "sortier",
    "lint-staged:stylelint": "stylelint --config ./.stylelintrc.json",
    "lint-staged:test": "jest --findRelatedTests",
    "prepare": "husky",
    "start": "npm run clean && npm run build:server && concurrently \"npm:watch:server\" \"npm:start:server\" \"npm:watch:browser\"",
    "start:server": "node --watch -r dotenv/config ./dist/server/index.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "watch:browser": "vite",
    "watch:server": "npm run build:server -- --incremental --watch"
  },
  "version": "1.0.0",
  "type": "module"
}
